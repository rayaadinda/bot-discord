{
  "name": "hpz-crew-discord-bot",
  "version": "1.0.0",
  "description": "HPZ Crew Discord bot with Supabase integration for community management and point system",
  "main": "dist/index.js",
  "scripts": {
    "dev": "nodemon --exec ts-node src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "format": "prettier --write src/**/*.ts",
    "clean": "rm -rf dist",
    "deploy": "npm run build && npm start"
  },
  "keywords": ["discord", "bot", "supabase", "hpz-crew", "community", "typescript"],
  "author": "HPZ TV",
  "license": "MIT",
  "type": "commonjs",
  "dependencies": {
    "@supabase/supabase-js": "^2.75.1",
    "discord.js": "^14.23.2",
    "dotenv": "^17.2.3",
    "express": "^4.21.2",
    "node-cron": "^4.2.1",
    "winston": "^3.18.3"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^24.8.1",
    "@typescript-eslint/eslint-plugin": "^8.46.1",
    "@typescript-eslint/parser": "^8.46.1",
    "eslint": "^9.38.0",
    "nodemon": "^3.1.10",
    "prettier": "^3.6.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3"
  }
}
